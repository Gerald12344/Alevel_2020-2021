on.z = pos.z\n\n        this.grass = group\n        this.speed = speed\n        this.pos = pos\n    }\n\n    update() {\n        let geos = this.grass\n        geos.position.x += this.speed\n        if (geos.position.x > 400) {\n            geos.position.x = this.pos.x\n        }\n    }\n}\n"]},"metadata":{},"sourceType":"module"}