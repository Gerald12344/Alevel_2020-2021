t = new MeshPhongMaterial({\n            color: 'white',\n            flatShading: true,\n        })\n\n        let cloud = new Mesh(\n            tuft1,\n            mat\n        )\n        let cloud1 = new Mesh(\n            tuft2,\n            mat\n        )\n        let cloud2 = new Mesh(\n            tuft3,\n            mat\n        )\n\n        group.add(cloud)\n        group.add(cloud1)\n        group.add(cloud2)\n\n        scene.add(group)\n        group.castShadow = true\n        group.receiveShadow = true\n        group.position.x = pos.x\n        group.position.y = pos.y\n        group.position.z = pos.z\n\n        this.grass = group\n        this.speed = speed   \n        this.pos = pos\n    }\n\n    update() {\n        let geos = this.grass\n        geos.position.x +=  this.speed\n        if(geos.position.x > 400){\n           geos.position.x = this.pos.x\n        }\n    }\n}\n"]},"metadata":{},"sourceType":"module"}