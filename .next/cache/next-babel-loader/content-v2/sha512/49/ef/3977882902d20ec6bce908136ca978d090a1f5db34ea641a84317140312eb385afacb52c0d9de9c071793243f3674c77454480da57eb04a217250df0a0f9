Airelone= await addGLBFile(\n            \"/Aircraft/BasicPlane/rightAirelone.glb\",\n            SceneToGet\n        );\n    } catch (e) {\n        console.log(e);\n        return false;\n    }\n    let group = new Group();\n    group.add(plane.scene);\n    group.add(blades.scene);\n    group.add(leftAirelone.scene)\n    group.add(rightAirelone.scene)\n\n    leftAirelone.scene.position.set(12.936,15.994,-59.264)\n    rightAirelone.scene.position.set(-12.495,15.888,-59.294)\n\n    //leftAirelone.scene.rotateZ(5)\n    blades.scene.position.set(0, 15, 22);\n    SceneToGet.add(group);\n    group.scale.set(0.05, 0.05, 0.05);\n    //console.log(blades.scene)\n    addToRenderSequence(\"Main\", () => {\n        blades.scene.rotateZ(0.3);\n        leftAirelone.scene.rotateX(0.005)\n        //group.rotateY(0.03)\n        //group.position.set(0,0, group.position.z+0.01)\n    });\n}\n"]},"metadata":{},"sourceType":"module"}